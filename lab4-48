#include <iostream>

using namespace std; 

int main()
{
    int l = 25;
    for (int a = 1; a <= l; ++a) 
    {
        for (int b = a; b <= l; ++b) 
        {
            for (int c = b; c <= l; ++c) 
            {
                if (a * a + b * b == c * c) 
                {
                    cout << a << ", " << b << ", " << c << endl;
                }
            }
        }
    }
    return 0;
}
